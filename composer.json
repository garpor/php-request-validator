{
    "version": "0.0.0",
    "name": "garpor/php-request-validator",
    "description": "A PHP library for validating request data.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-mbstring": "*"
	},
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Garpor\\PhpRequestValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Garpor\\PhpRequestValidator\\tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/tests/",
            "/.dockerignore",
            "/compose.yaml",
            "/Dockerfile",
            "/Makefile",
            "/phpunit.xml"
        ]
    }
}
